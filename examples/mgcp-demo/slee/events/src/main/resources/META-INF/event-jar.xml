<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.0//EN"
                           "http://java.sun.com/dtd/slee-event-jar_1_0.dtd">
<event-jar>
	<event-definition>
		<description>Fired when conference starts</description>
		<event-type-name>
			org.mobicents.mgcp.demo.events.INITIATE
		</event-type-name>
		<event-type-vendor>org.mobicents</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>
			org.mobicents.mgcp.demo.events.CustomEvent
		</event-class-name>
	</event-definition>

	<event-definition>
		<description>Fired when conference ends</description>
		<event-type-name>
			org.mobicents.mgcp.demo.events.TERMINATE
		</event-type-name>
		<event-type-vendor>org.mobicents</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>
			org.mobicents.mgcp.demo.events.CustomEvent
		</event-class-name>
	</event-definition>
</event-jar>
